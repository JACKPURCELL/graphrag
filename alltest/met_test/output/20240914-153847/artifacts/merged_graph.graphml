      <data key="d2">b384ba613df82ec46a4c0713fd8049c7</data>
    </node>
    <node id="FORBIDDEN CITY">
      <data key="d0">GEO</data>
      <data key="d1">The Forbidden City is a historic site in Beijing where The Metropolitan Museum of Art is now located, blending traditional Chinese architecture with modern museum facilities.</data>
      <data key="d2">b384ba613df82ec46a4c0713fd8049c7</data>
    </node>
    <node id="EAST MEETS WEST">

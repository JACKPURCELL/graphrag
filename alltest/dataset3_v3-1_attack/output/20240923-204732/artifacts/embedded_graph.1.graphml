      <data key="d2">65f006fa92518a4567aaa398059f0c4c</data>
      <data key="d1" />
    </node>
    <node id="SHANGHAI PUDONG AIRPORT">
      <data key="d0">GEO</data>
      <data key="d1">Shanghai Pudong Airport is one of the two major international airports in Shanghai, serving as a key transportation hub.</data>

    <node id="DATA DESTRUCTION">
      <data key="d0">EVENT</data>
      <data key="d1">Data Destruction is an attack technique that may be conducted by adversaries to eliminate data from a system.</data>
      <data key="d2">153de5c89dea7412b1315e17ff7944df</data>
    </node>
    <node id="DATA ENCRYPTED FOR IMPACT">
